.tqb-city-picker
  background-color white
  position fixed
  top 0
  left 0
  right 0
  bottom 0
  display flex
  flex-direction column

  &.static
    position static
    max-width 375px
    height 400px
    border 1px solid silver

  @import "_header.styl"
  @import "_container.styl"
  @import "_list.styl"